# Personal account
Host gh_personal
	HostName github.com
	PreferredAuthentications publickey
	IdentityFile /home/anthony/.ssh/personal_id_rsa
	User git

# Work account
Host gh_work
	HostName github.com
	PreferredAuthentications publickey
	IdentityFile /home/anthony/.ssh/personal_id_rsa
	User git

# Combined account
Host gh
    HostName github.com
    PreferredAuthentications publickey
    IdentityFile /home/anthony/.ssh/personal_id_rsa
    User git

# Spaceback Staging
Host spbk-db-staging
	HostName ec2-54-244-61-226.us-west-2.compute.amazonaws.com
	IdentityFile /home/anthony/.ssh/spbk-ops.pem
	ForwardAgent yes
	User ec2-user

